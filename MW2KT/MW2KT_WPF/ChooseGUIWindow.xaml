<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MW2KT_WPF"
        xmlns:UI="clr-namespace:MW2KT_WPF.UI" x:Class="MW2KT_WPF.ChooseGUIWindow"
        mc:Ignorable="d"
        Title="ChooseGUIWindow" Height="300" Width="300">
    <Grid>
        <Button Content="Scoreboard GUI (Same as WinForms)" Margin="10,10,10,0" VerticalAlignment="Top" Click="btnScoreboardGUI_Click"/>
        <Button Content="Table GUI" Margin="10,35,10,0" VerticalAlignment="Top" Click="btnTableGUI_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="70,121,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

        <UI:MW2Label x:Name="mw2lbl" HorizontalAlignment="Left" Height="27" Margin="70,190,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
